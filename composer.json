{

    "name": "tiknil/svg-sprite",
    "description": "Laravel package to create a sprite from a folder with svg",
    "keywords": [
        "tiknil",
        "svg-sprite"
    ],
    "version": "1.0.0",
    "bin": [
        "bin/svg-sprite"
    ],
    "homepage": "https://github.com/tiknil/svg-sprite",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Giorgio Balduzzi",
            "email": "balduzzi.giorgio@tiknil.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "ext-dom": "*"
    },
    "autoload": {
        "psr-4": {
            "Tiknil\\SvgSprite\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tiknil\\SvgSprite\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
